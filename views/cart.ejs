<% if (locals.cart) { %> 
  <% var checkout=[] %>
    <% cart.forEach(order=>{ %>                                           
           <%checkout.push(order.item)%>
  <%})%>

  <%var list = {}%>
  <%for (var i = 0; i<checkout.length; i++){%>
    <% if (list.hasOwnProperty(checkout[i])){%>
            <%list[checkout[i]] = list[checkout[i]]+1%>
        <%}else{%>
            <%list[checkout[i]]='1'%>
    <%}%>
  <% }%>

<%for (var j = 0; i<list.length; i++){%>
  <%list[Object.keys(list)[j]]=list[checkout[j]].length%>
  <%}%>
<%=JSON.stringify(list)%>
<%=list[checkout[0]].length%>
<%={(Object.keys(list)[j]):(list[checkout[0]].length)}%>
<%}%>

